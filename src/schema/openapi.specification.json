{
  "openapi": "3.0.0",
  "info": {
    "title": "Authentication service",
    "version": "1.0.0"
  },
  "paths": {
    "/users": {
      "get": {
        "summary": "Get a list of users",
        "description": "Retrieve a list of user entities.",
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/User"
                  }
                }
              }
            }
          },
          "500": {
            "description": "Internal Server Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#components/schemas/error"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "User": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "default": "johndoe@gmail.com"
          },
          "password": {
            "type": "string",
            "default": "securePassword123"
          },
          "rememberMe": {
            "type": "boolean"
          },
          "firstName": {
            "type": "string",
            "default": "john"
          },
          "lastName": {
            "type": "string",
            "default": "Doe"
          },
          "tcAgreed": {
            "type": "boolean"
          },
          "firstSignOn": {
            "type": "boolean",
            "default": false
          },
          "createdAt": {
            "type": "string",
            "format": "date-time"
          },
          "updatedAt": {
            "type": "string",
            "format": "date-time"
          }
        }
      },
      "Error": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string"
          }
        }
      },
      "error": {
        "type": "object",
        "properties": {
          "status": {
            "type": "integer",
            "default": 123
          },
          "message": {
            "type": "string",
            "default": "Something went wrong"
          },
          "error": {
            "type": "object"
          }
        }
      }
    }
  },
  "tags": []
}
